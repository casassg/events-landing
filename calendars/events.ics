---
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//{{site.name}}//Events//EN
X-WR-CALNAME:{{site.name}} calendar
CALSCALE:GREGORIAN
BEGIN:VTIMEZONE
TZID:America/Denver
X-LIC-LOCATION:America/Denver
BEGIN:DAYLIGHT
TZOFFSETFROM:-0700
TZOFFSETTO:-0600
TZNAME:MDT
DTSTART:19700308T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=2SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0600
TZOFFSETTO:-0700
TZNAME:MST
DTSTART:19701101T020000
RRULE:FREQ=YEARLY;BYMONTH=11;BYDAY=1SU
END:STANDARD
END:VTIMEZONE
{% for post in site.data.events %}{% unless post.date == null %}BEGIN:VEVENT
DTSTART;TZID=America/Denver:{{ post.date | date: "%Y%m%dT%H%M%S" }}
DTEND;TZID=America/Denver:{{ post.end-date | date: "%Y%m%dT%H%M%S" }}
DTSTAMP;TZID=America/Denver:{{ post.date | date: "%Y%m%dT%H%M%S" }}
UID:events-{{ post.date | date: "%Y%m%d-%H%M" }}@hackcu.org
LOCATION:{{ post.location }}
URL:{{post.url}}
SUMMARY:{{ post.name }}
END:VEVENT
{% endunless %}{% endfor %}END:VCALENDAR